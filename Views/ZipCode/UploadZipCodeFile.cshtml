@model SearchDataCollection.Models.ExcelUploadViewModel

@{
    ViewData["Title"] = "Import ZipCode File";
}

<h2>Importar Zip Code</h2>

<form method="post" enctype="multipart/form-data" asp-controller="ZipCode" asp-action="UploadZipCodeFile">
    <div class="form-group row mb-2">
        <label for="file" class="col-sm-2 col-form-label">Seleccionar archivo CSV</label>
        <div class="col-sm-10">
            <input type="file" class="form-control" id="file" name="file">
        </div>
    </div>
    <div class="text-center">
        <button type="submit" class="btn btn-primary">Importar</button>
    </div>
</form>


